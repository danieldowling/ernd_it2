<h1>Log In!</h1>

<%= form_for :login do |f| %>

	<div class="form-group">
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>

	<div class="form-group">
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>

	<div class="form-group">
		<%= f.submit 'Log In' %>
	</div>

	<%= link_to "Go Back", home_path %>

<% end %> 