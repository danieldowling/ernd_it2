<h1>Add a Post Here!</h1>

<%= form_for @posts do |f| %>

	<div>
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>


	<div>
		<%= f.label :image_url %>
		<%= f.file_field :image_url %>
	</div>

	<%= f.submit "Add Post" %>

	<%= link_to "Back to Home Page", posts_path %>

<% end %>